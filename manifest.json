{
    "manifest_version": 3,
    "name": "Grey Google Background",
    "description": "Changes google background",
    "version": "0.1",
    "icons": {
        "16": "/Images/Icon_16.png",
        "48": "/Images/Icon_48.png",
        "128": "/Images/Icon_128.png"
    },
    "action": {
        "default_icon": {
            "16": "/Images/Icon_16.png",
            "48": "/Images/Icon_48.png",
            "128": "/Images/Icon_128.png"
        },
        "default_popup": "popup.html",
        "default_title": "Options"
    },
    "content_scripts": [{
        "matches": ["*://www.google.com/*"],
        "css": ["bckg.css"]
    }]
  }